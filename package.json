{
  "name": "divvy",
  "version": "1.5.0",
  "description": "Budgeting App",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tausani-ah-chong/myBudget.git"
  },
  "main": "server/index.js",
  "author": "Tausani Ah Chong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tausani-ah-chong/myBudget/issues"
  },
  "homepage": "https://github.com/tausani-ah-chong/myBudget#readme",
  "scripts": {
    "client": "webpack serve --config ./client/webpack.config.js --hot",
    "dev": "run-p dev:client dev:server",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server",
    "start": "node server",
    "build": "npm run webpack",
    "server": "node server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:delete": "rm server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed",
    "postinstall": "run-s db:migrate db:seed",
    "test": "jest --watchAll",
    "test:watch": "jest --watchAll",
    "webpack": "webpack --config ./client/webpack.config.js",
    "lint": "eslint --ext .js,.jsx ."
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": "eda",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "knex": "^0.21.17",
    "sqlite3": "^5.0.0",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "7.9.0",
    "eslint-config-eda": "0.1.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "supertest": "^6.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
